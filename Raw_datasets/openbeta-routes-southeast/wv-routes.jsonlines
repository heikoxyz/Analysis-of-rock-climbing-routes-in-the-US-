{"route_name": "P-1", "grade": {"YDS": "5.7", "French": "5a", "Ewbanks": "15", "UIAA": "V+", "ZA": "13", "British": "MVS 4b"}, "safety": "", "type": {"trad": true}, "fa": "Paul Sullivan", "description": ["Climb up the flakes."], "location": ["Just right of Sassafras"], "protection": ["Hollow sounding flacks down low, but the protection gets better near the top. Run out the top (about 10 feet) on easy 4th class terrain."], "metadata": {"left_right_seq": "3", "parent_lnglat": [-80.8663, 37.478], "parent_sector": "08. Iceberg Area", "mp_route_id": "109197578", "mp_sector_id": "106535958", "mp_path": "Southern WV|Bozoo|08. Iceberg Area"}}
