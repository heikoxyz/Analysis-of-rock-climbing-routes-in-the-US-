{"route_name": "Lance Armstrong (backside)", "grade": {"YDS": "V4", "Font": "6B"}, "safety": "", "type": {"boulder": true}, "fa": "unknown", "description": ["Start on rail and move up and right to easier top-out"], "location": ["The right hand side of the backside of boulder"], "protection": ["Pad and a spotter"], "metadata": {"left_right_seq": "1", "parent_lnglat": [-81.36137, 35.17529], "parent_sector": "TMI Boulder", "mp_route_id": "107815099", "mp_sector_id": "107815070", "mp_path": "3. Piedmont Region|Crowders Mountain|Dixon School Boulders|TMI Boulder"}}
