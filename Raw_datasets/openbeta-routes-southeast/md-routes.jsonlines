{"route_name": "Applause", "grade": {"YDS": "5.7", "French": "5a", "Ewbanks": "15", "UIAA": "V+", "ZA": "13", "British": "MVS 4b"}, "safety": "PG", "type": {"trad": true, "tr": true}, "fa": "unknown", "description": ["Start at the small alcove in the wall to the left (facing the crag) of Rocky Road. Climb out of the alcove by traversing left and using the two large cracks in the face."], "location": ["Same area as Rocky Road. about 15 left of the climb 'Rocky Road'"], "protection": ["TR: may need some extra chocks.."], "metadata": {"left_right_seq": "0", "parent_lnglat": [-77.2484, 38.99483], "parent_sector": "Rocky Island - North End", "mp_route_id": "110524429", "mp_sector_id": "109581434", "mp_path": "Great Falls - Maryland Side|Rocky Islands, The|Rocky Island|Rocky Island - North End"}}
{"route_name": "Nail trim", "grade": {"YDS": "V0", "Font": "4"}, "safety": "", "type": {"boulder": true}, "fa": "unknown", "description": ["Start on the far right side of the boulder, and traverse left across a good rail and then good holds near the top. Top-out far left of boulder."], "location": "", "protection": ["Pad"], "metadata": {"left_right_seq": "3", "parent_lnglat": [-76.85215, 39.16033], "parent_sector": "Thumbnail boulder.", "mp_route_id": "120053244", "mp_sector_id": "120053234", "mp_path": "Murray Hill Road Boulders|North side of river|Thumbnail boulder."}}
{"route_name": "Cuticle", "grade": {"YDS": "V2", "Font": "5+"}, "safety": "", "type": {"boulder": true}, "fa": "unknown", "description": ["Start on side pulls on the far left of the slab, work across good holds and top out the arete. Finish same as the quick."], "location": ["Start on the far left of the slab face on the left/back of boulder."], "protection": "", "metadata": {"left_right_seq": "0", "parent_lnglat": [-76.85215, 39.16033], "parent_sector": "Thumbnail boulder.", "mp_route_id": "120053309", "mp_sector_id": "120053234", "mp_path": "Murray Hill Road Boulders|North side of river|Thumbnail boulder."}}
