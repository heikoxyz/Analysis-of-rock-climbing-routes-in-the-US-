{"route_name": "Dream The Impossible", "grade": {"YDS": "5.10c", "French": "6b", "Ewbanks": "20", "UIAA": "VII", "ZA": "20", "British": "E2 5b"}, "safety": "", "type": {"sport": true}, "fa": "Greg Zamule", "description": ["Start on the face right of the aerie. Move left onto the arete. Climb the arete and left face directly to the top."], "location": ["Far end of the Snip Block. Follows arete. Route #6 on Snip Block on 2019 guide."], "protection": ["3 bolts"], "metadata": {"left_right_seq": "5", "parent_lnglat": [-79.58151, 39.92033], "parent_sector": "Snip Block", "mp_route_id": "106605880", "mp_sector_id": "106455841", "mp_path": "Southwestern Highlands|Lost Crag, The|Snip Block"}}
{"route_name": "The Corner", "grade": {"YDS": "5.10b", "French": "6a+", "Ewbanks": "19", "UIAA": "VII-", "ZA": "19", "British": "E2 5b"}, "safety": "", "type": {"sport": true}, "fa": "Bob Coblenz, Tim Anderson", "description": ["Climb up through the easy overhanging corner to surmount the face. Climb the face using the arete to the right and face holds to your left to the top."], "location": ["Arete on the Snip Block. Route #1 on Snip Block on 2019 guide."], "protection": ["3 bolts"], "metadata": {"left_right_seq": "0", "parent_lnglat": [-79.58151, 39.92033], "parent_sector": "Snip Block", "mp_route_id": "106605869", "mp_sector_id": "106455841", "mp_path": "Southwestern Highlands|Lost Crag, The|Snip Block"}}
