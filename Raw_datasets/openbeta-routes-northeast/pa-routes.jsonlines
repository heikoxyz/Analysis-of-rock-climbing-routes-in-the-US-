{"route_name": "Austin's Pockets", "grade": {"YDS": "5.10a", "French": "6a", "Ewbanks": "18", "UIAA": "VI+", "ZA": "18", "British": "E1 5a"}, "safety": "", "type": {"sport": true}, "fa": "Climberdude", "description": ["Start on Slabadelic. Split right and follow pockets to corner. Then go up to share anchors with Slabadelic or traverse right to anchors on Dr Evil's cat."], "location": ["Same as Slabadelic"], "protection": ["Bolts"], "metadata": {"left_right_seq": "1", "parent_lnglat": [-76.40806, 39.93189], "parent_sector": "4.0 Austin Powers Buttress", "mp_route_id": "108813288", "mp_sector_id": "108805233", "mp_path": "South Central PA|Safe Harbor|Safe Harbor North|4.0 Austin Powers Buttress"}}
