{"route_name": "Whooley Roof", "grade": {"YDS": "V3", "Font": "6A"}, "safety": "", "type": {"boulder": true}, "fa": "Matt Whooley", "description": ["Start sitting underneath the roof with one hand on either side compressing the feature. Climb out of the roof and top out."], "location": ["Left side of the main cluster at the top of the white trail."], "protection": ["pads"], "metadata": {"left_right_seq": "0", "parent_lnglat": [-72.89626, 41.42665], "parent_sector": "Top of the White Trail", "mp_route_id": "112340479", "mp_sector_id": "111789694", "mp_path": "CT Bouldering|Sleeping Giant|Between the Chin and Chest|Top of the White Trail"}}
{"route_name": "Whooley Mammoth", "grade": {"YDS": "V3", "Font": "6A"}, "safety": "", "type": {"boulder": true}, "fa": "unknown", "description": ["Start in the depths below Whooley roof, left hand on an incut ledge in the left face, right hand overhead in a crack. Move up the sharp arete to the right (left side of Whooley roof) and then transition to an obvious jug on the left face. Easy top out."], "location": ["Top of the white trail, in the corner formed by Whooley roof and the face to the left."], "protection": ["Pads"], "metadata": {"left_right_seq": "1", "parent_lnglat": [-72.89626, 41.42665], "parent_sector": "Top of the White Trail", "mp_route_id": "114157291", "mp_sector_id": "111789694", "mp_path": "CT Bouldering|Sleeping Giant|Between the Chin and Chest|Top of the White Trail"}}
{"route_name": "Warm up to the warm up to the warm up", "grade": {"YDS": "V0", "Font": "4"}, "safety": "", "type": {"boulder": true}, "fa": "unknown", "description": ["Starts on the large jug and climbs right."], "location": ["Same boulder as the Warm up to the warm up, but on the right side."], "protection": ["pads."], "metadata": {"left_right_seq": "11", "parent_lnglat": [-72.89626, 41.42665], "parent_sector": "Top of the White Trail", "mp_route_id": "112343256", "mp_sector_id": "111789694", "mp_path": "CT Bouldering|Sleeping Giant|Between the Chin and Chest|Top of the White Trail"}}
